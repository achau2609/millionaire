      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_eFx5g5LgEe6uQoJcvkG_wg" name="pairCorrectLifeline" specification="_eBZIwJLgEe6uQoJcvkG_wg">
        <ownedParameter xmi:id="_eFx5hJLgEe6uQoJcvkG_wg" name="qInput">
