     for (int i = 0; i &lt; votes.length; i++) {&#xD;
     	int minimumVotes = i == question.getCorrectIndex()? 60 : 0;&#xD;
     	// correct answer has at least 60 votes;&#xD;
         votes[i] = random.nextInt(41) + minimumVotes;&#xD;
     }&#xD;
